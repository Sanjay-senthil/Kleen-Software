{
  "affected": {
    "defaultBase": "develop"
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "cloud",
  "implicitDependencies": {
    "nx.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "workspace.json": "*",
    ".eslintrc.json": "*"
  },
  "npmScope": "kleeen",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "e2e", "lint", "test"]
      }
    }
  },
  "generators": {
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    }
  }
}
